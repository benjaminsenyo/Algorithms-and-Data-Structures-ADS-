    "def sum_of_two(nums, target):\n",
    "    for i in range(len(nums)):\n",
    "        for j in range(i+1,len(nums)):\n",
    "            if i +j ==target:\n",
    "                re = print(f\" {i} and {j} and {nums[i]} and {nums[j]}\")\n",
    "                return re"
   ]
